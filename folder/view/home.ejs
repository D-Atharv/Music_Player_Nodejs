<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Music Player</title>
    <link href="/homepageStyle.css" rel="stylesheet" />

  </head>

  <body>
    <header class="header">
      <nav class="navbar">
        <a href="#">
          <img src="LOGO.jpg" alt="img" class="logo" />
          <h1 class="logo_name">MUSIC</h1>
        </a>
        <button class="login_btn">LOGOUT</button>
      </nav>
    </header>
    <!-- music -->
    <div class="music_box">
      <div class="container">
        <div class="top">
          <img src="back-svgrepo-com.svg" alt="back" class="back_btn top_img" />
          <img
            src="menu-svgrepo-com.svg"
            alt="drop down"
            class="drop_down top_img"
          />
        </div>
        <a href="album_image.html">
          <div>
            <img
              src="Album Cover Art.jpeg"
              alt="album_cover"
              class="album_img "
            />
          </div>
        </a>
        <div class="song_name">
          <h1>Moon Song</h1>
        </div>
        <div class="artist_name">
          <h1>Gregory Alan Isakov</h1>
        </div>
        <div class="container_bottom">
          <!-- <input type="range" name="range" min="0" max="100" /> -->
            <audio controls muted loop class="audio">
              <source src="That Moon Song.mp3" type="audio/mpeg" />
              <source src="That Moon Song.mp3" type="audio/wav" />
            </audio>
          </div>
          <div class="bottom">
            <ul class="play_options">
              <li id="one"><img src="previous-999-svgrepo-com.svg"></li>
              <li id="two" ><img src="play-svgrepo-com.svg"></li>
              <li id="four" ><img src="media-pause-svgrepo-com.svg"></li>
              <li id="three"><img src="next-998-svgrepo-com.svg"></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <!-- music -->
    <footer>
      <div class="footer_div">
        <marquee>Music Player</marquee>
      </div>
    </footer>

    <script src="script.js"></script>
  </body>
</html>
